{
  "name": "bauerquotes.com",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/bdebicki/bauerquotes.com"
  },
  "scripts": {
    "build": "rm -rf ./dist/* && webpack --mode production",
    "watch": "webpack-dev-server --content-base public",
    "eslint": "eslint ./src/js --ext .js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "if $PRE_COMMIT_HOOK; then lint-staged; fi",
      "pre-push": "if $PRE_PUSH_HOOK; then eslint ./src --ext .js --ext .jsx; fi"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "browserslist": "^4.12.0",
    "css-loader": "^3.6.0",
    "eslint": "7.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.21.2",
    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "less-loader": "^6.1.1",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
